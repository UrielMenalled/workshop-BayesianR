# Set default chunk options:
# - show code
# - show plots
# - hide code results (echo TRUE, fig.show = 'hold', results='hide')
knitr::opts_chunk$set(
  echo = TRUE,
  results = "hide",
  message = FALSE,  # hide messages
  warning = FALSE, # hide warnings
  fig.show = "asis",
  dev = "png",
  fig.path = 'figures/'
)

